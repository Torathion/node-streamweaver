{
  "name": "streamweaver",
  "version": "1.0.0",
  "description": "Extended promises with a lot of vital features. The AIO Promise class.",
  "types": "./index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "node esbuild.config.js",
    "lint": "eslint src --fix",
    "format": "npx prettier --write */**.ts",
    "test": "vitest"
  },
  "keywords": [
    "promise",
    "abort",
    "interrupt",
    "timeout",
    "data structure",
    "utility"
  ],
  "author": "Torathion",
  "homepage": "https://github.com/Torathion/streamweaver",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Torathion/streamweaver.git"
  },
  "bugs": {
    "url": "https://github.com/Torathion/streamweaver/issues"
  },
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@rollup/rollup-win32-ia32-msvc": "^4.40.2",
    "@types/node": "^22.15.18",
    "@vitest/coverage-v8": "^3.1.3",
    "esbuild": "^0.25.4",
    "eslint": "^9.26.0",
    "eslint-config-shiny": "^4.2.1",
    "jiti": "^2.4.2",
    "numbro": "^2.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "compresso": "^1.4.0",
    "string-width": "^7.2.0",
    "typestar": "^1.11.0"
  },
  "engines": {
    "node": ">=21.0.0"
  }
}
